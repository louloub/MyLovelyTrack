tion: row;\n        align-items: center;\n        padding: 1.5rem;\n    }\n    .links-container>a {\n        margin-right: 0.4rem;\n        margin-bottom: 0;\n    }\n    .logos-container {\n        display: flex;\n        align-items: center;\n    }\n}\n\n/* End Footer Lisa-Lou */\n\n\n\n/* Start actus Lisa-Lou */\n\nfigure > img {\n    width: 100%;\n}\n\n.header-article {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    text-align: center;\n}\n\n.header-article > h2 {\n    font-size: 3rem;\n    font-weight: bold;\n    padding: 1rem;\n}\n\narticle > p {\n    text-align: center;\n    padding: 1.5rem;\n}\n\n.header-article > h3 {\n    font-size: 2rem;\n    padding: 1rem;\n}\n\n.header-article > h4 {\n    font-size: 1rem;\n    padding: 1.5rem;\n    border-bottom: solid black;\n}\n\nfigcaption {\n    font-size: 0.3rem;\n    padding-bottom: 3rem;\n}\n\n.article-layout-image-right {\n    display: flex;\n    padding: 3em;\n}\n\n.article-layout-image-right > p {\n    text-align: start;\n}\n\n.content-article {\n    padding: 3em;\n}\n\n.layout-article {\n    text-align: start;\n}\n\n.pic-harold {\n    padding-bottom: 2rem;\n    padding-left: 2rem;\n    align-self: center;\n}\n\n.agenda-link {\n    padding: 0%;\n}\n\n\n.logos-socials-article {\n    display: flex;\n    justify-content: center;\n    padding-bottom: 2rem;;\n    border-bottom: double black;\n}\n\n.embed-youtube {\n    display: flex;\n    justify-content: center;\n}\n\n\n/* End actus Lisa-Lou */"},"lineCount":219}},"error":null,"hash":"ea8ce679bc62f9f4e7a82b53f1f9f4b1","cacheData":{"env":{}}}